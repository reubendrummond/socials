generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Customer {
  id    Int    @id @default(autoincrement())
  name  String
  email String @unique
  age   Int
  title Titles
}

model Item {
  id   Int    @id @default(autoincrement())
  name String
  cost Float
}

enum Titles {
  Mr
  Mrs
  Ms
  Miss
  Lord
}
